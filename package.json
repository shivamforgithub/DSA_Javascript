{
  "name": "common-module-code",
  "version": "0.0.1",
  "description": "Lending API For LMF",
  "author": "LMF",
  "license": "ISC",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development tsc-watch -p tsconfig.build.json --onSuccess \"node -r dotenv/config dist/src/main.js\"",
    "prestart:prod": "npm install && npm run build",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config dist/src/main.js",
    "prestart:staging": "npm install && npm run build",
    "start:staging": "cross-env NODE_ENV=test node -r dotenv/config dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=development jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:cov": "cross-env NODE_ENV=development jest --coverage",
    "test:cov:staging": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=e2e-test jest --detectOpenHandles --maxWorkers=1 --config ./e2e-test/jest-e2e.json --coverage --forceExit",
    "test:e2e:staging": "cross-env NODE_ENV=test jest --detectOpenHandles --maxWorkers=1 --config ./e2e-test/jest-e2e.json --coverage",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/db/typeorm.config.ts",
    "migration:generate": "npm run typeorm -- -d ./src/db/typeorm.config.ts migration:generate ./src/db/migrations/%npm_config_name%",
    "migration:create": "npm run typeorm -- migration:create ./src/db/migrations/%npm_config_name%",
    "migration:revert": "npm run typeorm -- -d ./src/db/typeorm.config.ts migration:revert",
    "migration:show": "npm run typeorm -- migration:show -d ./src/db/typeorm.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
    "@aws-sdk/client-sns": "^3.385.0",
    "@aws-sdk/client-sqs": "^3.385.0",
    "@aws-sdk/s3-request-presigner": "^3.430.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/event-emitter": "^2.0.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "async-retry": "^1.3.3",
    "axios": "^1.4.0",
    "blob": "^0.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cls-hooked": "^4.2.2",
    "cls-rtracer": "^2.6.3",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cron": "^2.3.1",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.1.4",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "maskdata": "^1.2.6",
    "moment": "^2.29.4",
    "nestjs-command": "^3.1.4",
    "newrelic": "^11.7.0",
    "nodemailer": "^6.9.4",
    "papaparse": "^5.4.1",
    "pg": "^8.11.0",
    "redis": "^4.6.7",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "sqs-consumer": "^7.2.2",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.3.0",
    "@types/nodemailer": "^6.4.9",
    "@types/papaparse": "^5.3.8",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.20.0",
    "typescript": "^4.1.6"
  },
  "main": "main.ts",
  "directories": {
    "test": "test"
  }
}
